 <div class="search field search_<%= field.name %>">
   <%= label_tag field.label , field.name ,{:class => "main_label" } %>
      
   <span class="date">
        <label>Between: </label>
        <input id="<%= field.name %>" class="datepicker" type="text" name="query[<%=field.name%>][from]" size="32" />
      </span>
      <span class="date">
        <label>and:</label>
        <input class="datepicker" type="text" name="query[<%=field.name%>][to]" size="32" />
      </span>
    
    </div>
                <% head_style "rails_admin/jquery.ui.timepicker.css" %>
                <% head_javascript "rails_admin/jquery.ui.timepicker.js" %>
                <% head_javascript "rails_admin/ra.datetimepicker.js" %>
                <% head_javascript do %>
                  $j(document).ready(function($){
                    $("input.datepicker").datepicker();
                  });
                <% end %>
