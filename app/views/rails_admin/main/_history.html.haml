%br
%h4= "#{month_names[month - 1]} #{year} #{t("admin.history.name").downcase}"
%table.zebra-striped
  %thead
    %tr
      %th.user t("admin.history.user")
      %th.changes= t("admin.history.changes")
  %tbody
    - history.each do |t|
      %tr
        %td= t.try :username
        %td= history_output(t)
