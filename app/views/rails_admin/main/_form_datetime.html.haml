= field_wrapper_for(form, field) do
  = form.send field.view_helper, field.method_name, field.html_attributes

- head_style "rails_admin/jquery.ui.timepicker.css"
- head_javascript "rails_admin/jquery.ui.timepicker.js"
- head_javascript "rails_admin/ra.datetimepicker.js"
- head_javascript do
  :plain
    $j(document).ready(function($){
      $("##{field.dom_id}").datetimepicker(#{field.js_plugin_options.html_safe});
    });
